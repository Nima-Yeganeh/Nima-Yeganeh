<% pages = pages || [] %>

<% if (pages.length) { %>

	<div id="app-header"></div>
	<div id="app-sidebar"></div>

	<div class="page-main-wrap">
		<div class="page-main-inner">
			<div class="custom-page-wrapper">
				<h2>Sitemap</h2>
				<hr>
				<h3>Pages</h3>
				<ol>
					<% for (p of pages) { %>
					<li><a href="./<%= p %>.html"><%= p %></a></li>
					<% } %>
				</ol>
			</div>
		</div>
	</div>
	
<% } %>